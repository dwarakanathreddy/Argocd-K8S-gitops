apiVersion: argoproj.io/v1alpha1
kind: Application
metadata:
  name: nginx-argo
spec:
  project: default
  destination:
    namespace: default
    server: https://kubernetes.default.svc
  source:
    directory:
      include: 'argo-examples/*'
    path: argo-examples
    repoURL: https://github.com/dwarakanathreddy/Kubernetes-RunBook.git
    targetRevision: main